{
	"name": "swyxkit",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"start": "concurrently --names \"APP,HISTOIRE\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev\" \"npm run story:dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"test": "playwright test",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@histoire/plugin-svelte": "^0.12.4",
		"@playwright/test": "^1.29.2",
		"@svelte-put/toc": "^2.0.1",
		"@sveltejs/adapter-auto": "^1.0.1",
		"@sveltejs/adapter-netlify": "^1.0.3",
		"@sveltejs/kit": "1.1.1",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gray-matter": "^4.0.3",
		"histoire": "^0.12.4",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "~2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-format": "^4.0.1",
		"rehype-slug": "^5.1.0",
		"rehype-stringify": "^9.0.3",
		"remark-abbr": "^1.4.1",
		"remark-autolink-headings": "^7.0.1",
		"remark-frontmatter": "^4.0.1",
		"remark-github": "^11.2.4",
		"remark-html": "^15.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remark-sectionize": "^1.1.1",
		"remark-slug": "^7.0.1",
		"remark-toc": "^8.0.1",
		"rss": "^1.2.2",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.0.0",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"unified": "^10.1.2",
		"vfile": "^5.3.6",
		"vfile-reporter": "^7.0.4",
		"vite": "^4.0.4"
	},
	"engines": {
		"node": ">=16.14"
	},
	"type": "module",
	"dependencies": {
